@model IEnumerable<WebStore.Models.EmployeeModel>
@{
    ViewData["Title"] = "Index";
}

<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.Patronymic</td>
            <td>@item.SurName</td>
            <td>@item.Position</td>
            <td>@item.Age</td>
        </tr>
    }
</table>

